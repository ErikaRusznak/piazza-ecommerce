{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_REACT_APP_BACKEND_URL",
    "NEXT_PUBLIC_STRIPE_KEY",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY"
  ],
  "pipeline": {
    "dev": {
      "cache": false,
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "env": ["SOME_ENV_VAR", "API_KEY_VAR"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
